namespace org.acme.chainlec

participant Voter identified by passportID {
  o String passportID
}

participant HeadOfElection extends Voter {
}

participant Candidate extends Voter {
  o Integer numberOfVotes default = 0
}

asset Poll identified by pollID {
  o String pollID
  --> Candidate[] candidates
}

transaction Vote {
  --> Voter voter
  --> Candidate candidate
}
